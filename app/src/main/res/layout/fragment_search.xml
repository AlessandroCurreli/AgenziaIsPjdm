<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".SearchMain2Activity"
    style="@style/AppTheme"
    android:background="#1de9b6"
    android:orientation="vertical"
    >


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/title_message"
        android:layout_gravity="start"
        android:textColor="#3d5afe"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="70dp"
        android:textAppearance="?android:textAppearanceLarge"/>


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Immobili_1"
        android:text="@string/Appartamento"

        android:onClick="select_item"
        android:checked="false"
        android:layout_alignTop="@+id/Immobili_4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Immobili_2"
        android:text="@string/Villa"
        android:onClick="select_item"
        android:textColor="#000000"
        android:layout_below="@+id/Immobili_3"
        android:layout_alignLeft="@+id/Immobili_3"
        android:layout_alignStart="@+id/Immobili_3" />


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Immobili_3"
        android:onClick="select_item"
        android:layout_below="@+id/Immobili_1"
        android:textColor="#000000"
        android:text="@string/Monolocale" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Immobili_4"
        android:onClick="select_item"
        android:textColor="#000000"
        android:text="@string/Ufficio"
        android:layout_below="@+id/textView"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toEndOf="@+id/textView2" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Immobili_5"
        android:onClick="select_item"
        android:textColor="#000000"
        android:layout_below="@+id/Immobili_6"
        android:layout_alignLeft="@+id/Immobili_4"
        android:layout_alignStart="@+id/Immobili_4"
        android:text="@string/Negozio" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Immobili_6"
        android:onClick="select_item"
        android:layout_below="@+id/Immobili_4"
        android:layout_alignLeft="@+id/Immobili_4"
        android:layout_alignStart="@+id/Immobili_4"
        android:textColor="#000000"
        android:text="@string/Magazzino" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Immobili_7"
        android:onClick="select_item"
        android:layout_below="@+id/Immobili_2"
        android:layout_alignLeft="@+id/Immobili_2"
        android:layout_alignStart="@+id/Immobili_2"
        android:textColor="#000000"
        android:text="@string/Box" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Immobili_8"
        android:onClick="select_item"
        android:checked="false"
        android:layout_below="@+id/Immobili_5"
        android:layout_alignRight="@+id/Immobili_5"
        android:layout_alignEnd="@+id/Immobili_5"
        android:textColor="#000000"
        android:text="@string/Terreno" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/title_msg"
        android:id="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Immobili_7"
        android:textColor="#3d5afe"
        android:textAppearance="?android:textAppearanceLarge"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idprezzo1"
        android:id="@+id/idprezzo1"
        android:checked="false"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="select_item"
        android:textColor="#000000"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idPrezzo2"
        android:id="@+id/idPrezzo2"
        android:checked="false"
        android:onClick="select_item"
        android:textColor="#000000"
        android:layout_above="@+id/idPrezzo3"
        android:layout_alignLeft="@+id/Immobili_8"
        android:layout_alignStart="@+id/Immobili_8" />


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/idPrezzo3"
        android:checked="false"
        android:layout_below="@+id/idprezzo1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="select_item"
        android:textColor="#000000"
        android:text="@string/idPrezzo3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/idPrezzo2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#3d5afe"
        android:textAppearance="?android:textAppearanceLarge"
        android:text="@string/textView3" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idMq1"
        android:id="@+id/idMq1"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:checked="false"
        android:onClick="select_item"
        android:textColor="#000000"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idMq2"
        android:id="@+id/idMq2"
        android:checked="false"
        android:onClick="select_item"
        android:layout_below="@+id/idMq3"
        android:layout_alignLeft="@+id/idMq3"
        android:layout_alignStart="@+id/idMq3"
        android:textColor="#000000"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idMq4"
        android:id="@+id/idMq4"
        android:checked="false"
        android:onClick="select_item"
        android:layout_below="@+id/idMq1"
        android:layout_alignRight="@+id/idMq1"
        android:layout_alignEnd="@+id/idMq1"
        android:textColor="#000000"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idMq3"
        android:id="@+id/idMq3"
        android:layout_alignTop="@+id/idMq1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:checked="false"
        android:onClick="select_item"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idSearch"
        android:id="@+id/idButtonSearch"
        android:onClick="finalSelection"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/idAgenzia"
        android:layout_alignStart="@+id/idAgenzia"
        android:layout_marginBottom="23dp"
        android:textColor="#ff3d00"
        />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/idMyProperties"
        android:id="@+id/idProperty"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/idMq4"
        android:layout_alignStart="@+id/idMq4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agenzia"
        android:id="@+id/idAgenzia"
        android:layout_alignTop="@+id/idProperty"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>